{
	"rules": [
		{
			"rule": "convert_require",
			"current": {
				"name": "path",
				"sources": {
					"@src": "./src",
					"@iris": "./src/iris",
					"@launcher": "./src/launcher",
					"@settings": "./src/settings",
					"@theme": "./src/theme",
					"@utils": "./src/utils",
					"@vendor": "./vendor",
					"@pkg": "./roblox_packages"
				}
			},
			"target": {
				"name": "roblox",
				"rojo_sourcemap": "./sourcemap.json",
				"indexing_style": "find_first_child"
			}
		}
	]
}
